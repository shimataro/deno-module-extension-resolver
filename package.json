{
  "name": "deno-module-extension-resolver",
  "version": "0.2.0",
  "description": "NPM module to resolve import module extension for Deno",
  "bin": {
    "deno-module-extension-resolver": "bin/deno-module-extension-resolver"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "check-updates": "ncu",
    "verify": "run-p lint",
    "lint": "run-p lint:*",
    "lint:ts": "eslint ./src --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shimataro/deno-module-extension-resolver.git"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=5.6.0"
  },
  "author": "shimataro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shimataro/deno-module-extension-resolver/issues"
  },
  "homepage": "https://github.com/shimataro/deno-module-extension-resolver#readme",
  "devDependencies": {
    "@types/node": "14.14.25",
    "@typescript-eslint/eslint-plugin": "4.15.0",
    "@typescript-eslint/parser": "4.15.0",
    "eslint": "7.19.0",
    "npm-check-updates": "11.1.1",
    "npm-run-all": "4.1.5"
  },
  "dependencies": {
    "fast-glob": "3.2.5",
    "typescript": "4.1.4"
  }
}
